{
    "identifier": "mobile-capability",
    "windows": ["main"],
    "platforms": ["iOS"],
    "permissions": [
        "path:default",
        "event:default",
        "window:default",
        "app:default",
        "resources:default",
        "menu:default",
        "tray:default",
        "shell:allow-open",
        "shell:default",

         "nfc:allow-scan",
      "biometric:allow-authenticate",
      "barcode-scanner:allow-scan"
    ]
  }